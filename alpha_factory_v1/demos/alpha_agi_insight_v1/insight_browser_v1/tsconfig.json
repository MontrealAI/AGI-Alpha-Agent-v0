{
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@insight-src/*": ["../../../../src/*"]
    },
    "allowJs": true,
    "allowImportingTsExtensions": true
  },
  "include": ["src/**/*.ts", "src/**/*.js"]
}
